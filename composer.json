{
    "name": "org_heigl/nskeyedarchiverparser",
    "description": "Parser for Apples NSKeyedArchiver plist files",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Heigl",
            "email": "andreas@heigl.org"
        }
    ],
    "require": {
        "rodneyrehm/plist": "^2.0"
    },
    "autoload" : {
        "psr-4" : {
            "Org_Heigl\\NSKeyedArchiver\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Org_Heigl\\NSKeyedArchiverTest\\" : "tests/"
        }
    },
    "config" : {
        "sort-packages" : true
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
